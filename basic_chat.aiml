<aiml version="1.0.1" encoding="UTF-8">
<!-- basic_chat.aiml -->

    <category>
        <pattern>HELLO</pattern>
        <template>
            Hi!
        </template>
    </category>

    <category>
        <pattern>WHAT ARE YOU</pattern>
        <template>
            I'm a cat!
        </template>
    </category>

    <category>
        <pattern>HOW ARE YOU</pattern>
        <template>
            I'm fine, how are you?
        </template>
    </category>

    <category>
      <pattern>MY NAME IS *</pattern>
      <template>
          <think><set var="checkname"><get name = "username"/></set></think>
          <condition name="checkname">
              <li value="unknown">Nais to meet yu, <set name="username"><star/></set>. How old ar yu?</li>
              <li>Oh, hello there, <get name="username"/>. Nice to see you again!</li>
          </condition>

      </template>
   </category>

    <category>
      <pattern>I AM * YEARS OLD</pattern>
      <template>
         That's nais. what du you du? <think><set name="age"><star/></set></think>
      </template>

   </category>

    <category>
      <pattern>I AM A *</pattern>
      <template>
         Lief is hard, am I rite?<think><set name="occupation"><star/></set></think>
      </template>
   </category>


    <category>
      <pattern>MY DOGS NAME IS *</pattern>
      <template>
         That is interesting that you have a dog named <set name="dog"><star/></set>
      </template>
   </category>

   <category>
      <pattern>WHAT IS MY DOGS NAME</pattern>
      <template>
         Your dog's name is <get name="dog"/>.
      </template>
   </category>

    <category>
      <pattern>NAIS</pattern>
      <template>
         NAIIIISSSS
      </template>
   </category>

    <category>
      <pattern>clatite</pattern>
      <template>
         vreau
      </template>
   </category>
   
   
   <category>
        <pattern>NAME NOT GIVEN</pattern>
        <template>
            <random>
                <li>What's your name?</li>
                <li>Please tell me your name</li>
                <li>Who are you?</li>
            </random>
        </template>
    </category>

    <category>
        <pattern>AGE NOT GIVEN</pattern>
        <template>
            <random>
                <li>What's your age?</li>
                <li>Please tell me your age</li>
                <li>How old are you?</li>
            </random>
        </template>
    </category>

    <category>
        <pattern>OCCUPATION NOT GIVEN</pattern>
        <template>
            <random>
                <li>What's your occupation?</li>
                <li>Please tell me your occupation</li>
            </random>
        </template>
    </category>

</aiml>