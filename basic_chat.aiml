     <category>
        <pattern>*</pattern>
        <template>
            <condition name="age_category" value="child">
               <random>
                   <li>Do you go to school yet?</li>
                   <li>Do you like animals?</li>
                   <li>What do you like playing with your friends?</li>
                   <li>What's your favourite color?</li>
                   <li>Have you been listening to your parents?</li>
                   <li>Do you have any siblings?</li>
                   <li>What do you enjoy doing the most?</li>
                   <li>What would you like to become when you grow up?</li>
               </random>
            </condition>
        </template>
    </category>

